<div class="jumbotron header">
  <h1>Competitive</h1>
</div>

<div class="main-content">
  <h3>Please select division you are searching for.</h3>

  <table class="trade-items">
  <tr>
    <% [:highlander, :sixes, :medieval].each do |type| %>
      <td>
        <a href="#">
          <%= image_tag "100px_#{type}button.png" %>
          <span class="item-name"><%= type %></span>
        </a>
      </td>
    <% end %>
  </tr>
  </table>
</div>
